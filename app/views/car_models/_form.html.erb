<%= form_for @car_model do |f| %>
  <%= f.label :brand%>
  <%= f.text_field :brand%><br>

  <%= f.label :model%>
  <%= f.text_field :model%><br>

  <%= f.label :description%>
  <%= f.text_area :description%><br>

  <%= f.label :image_url%>
  <%= f.text_field :image_url%><br>

  <%= f.submit %>
<% end %>
