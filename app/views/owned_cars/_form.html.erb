<%= form_for @owned_car do |f| %>
  
  <%= f.label :car_model_id %>
  <%= f.collection_select :car_model_id, CarModel.all, :id, :car_name %>
  <%= f.label :year %>
  <%= f.text_field :year %>
  <%= f.label :price_per_day %>
  <%= f.text_field :price_per_day %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.hidden_field :user_id, value: session[:user_id] %>
  <%= f.submit %>
<% end %>