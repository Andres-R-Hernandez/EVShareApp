
<%= form_for @booking do |f| %>
  <%= f.label :booking_time %>
  <%= f.datetime_local_field :booking_time %>
  <%= f.label :owned_car_id %>
  <%= f.collection_select :owned_car_id, OwnedCar.all, :id, :car_full_name_with_price %>
  <%= f.hidden_field :user_id, value: session[:user_id] %>
  <%= f.submit %>
<% end %>
