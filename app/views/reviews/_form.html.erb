<%= form_for @review do |f| %>

  <%= f.label :booking_id %>
  <%= f.collection_select :booking_id, Booking.all, :id, :id %><br>
  <%= f.label :description %><br>
  <%= f.text_area :description %><br>
  <%= f.label :star_rating %>
  <%= f.number_field :star_rating %><br>
  <%= f.hidden_field :user_id, value: session[:user_id] %>
  <%= f.submit %>
<% end %>
