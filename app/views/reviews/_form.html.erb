<%= form_for @review do |f| %>
  
  <%= f.label :booking_id %>
  <%= f.collection_select :booking_id, Booking.all, :id, :booking_time %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :star_rating %>
  <%= f.number_field :star_rating %>
  <%= f.hidden_field :user_id, value: session[:user_id] %>
  <%= f.submit %>
<% end %>